<div id="<%= dom_id post %>">
  <p class="my-5">
    <strong class="block font-medium mb-1">Title:</strong>
    <%= post.title %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Category:</strong>
    <%= post.category %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Post:</strong>
    <%= post.post %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Author:</strong>
    <%= post.author %>
  </p>

</div>
